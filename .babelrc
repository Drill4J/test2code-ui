{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "useESModules": true,
        "regenerator": false
      }
    ],
    [
      "babel-plugin-styled-components",
      {
        "fileName": true,
        "cssNamespace": "#test2code"
      }
    ],
    "babel-plugin-macros",
    "@babel/plugin-transform-react-jsx"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": "current node"
          }
        ]
      ]
    }
  }
}
