  "version": "0.1.0-83",
  "main": "dist",
  "scripts": {
    "start": "npm run tailwind:css && webpack serve --port 7087",
    "start:standalone": "npm run tailwind:css && webpack serve --env standalone",
    "build": "npm run tailwind:css && webpack --mode=production",
    "analyze": "webpack --mode=production --env analyze",
    "lint": "eslint src --ext js,ts,tsx",
    "lint:fix": "eslint src --ext js,ts,tsx --fix",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "test": "cross-env BABEL_ENV=test jest",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "tailwind:css": "tailwind build src/tailwind.css -c tailwind.config.js -o src/index.css"
  },
